  },
  {
    "processorType": "DeleteFile",
    "enabled": true,
    "name": "Remove old log",
    "source": "demofiles/logs/old.log"
  },
  {
    "processorType": "WaitKey",
    "enabled": true,
    "name": "Completion notification",
    "message": "Cleanup complete. Press Ctrl+C to exit..."
  }
